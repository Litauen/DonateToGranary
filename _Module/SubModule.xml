<?xml version="1.0" encoding="UTF-8"?>
<Module xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/BUTR/Bannerlord.XmlSchemas/master/SubModule.xsd" >
  <Id value="$modulename$" />
  <Name value="$modulename$" />
  <Version value="v$version$" />
  <DefaultModule value="false" />
  <ModuleCategory value="Singleplayer"/>
  <ModuleType value="Community" />
  <DependedModules>
    <DependedModule Id="Native" DependentVersion="v1.1.2"/>
    <DependedModule Id="SandBoxCore" DependentVersion="v1.1.2"/>
    <DependedModule Id="Sandbox" DependentVersion="v1.1.2"/>
    <DependedModule Id="StoryMode" DependentVersion="v1.1.2"/>
  </DependedModules>
  <!-- Community Dependency Metadata -->
  <!-- https://github.com/BUTR/Bannerlord.BUTRLoader#for-modders -->
  <DependedModuleMetadatas>
    <DependedModuleMetadata id="Native" order="LoadBeforeThis" version="1.1.2"/>
    <DependedModuleMetadata id="SandBoxCore" order="LoadBeforeThis" version="1.1.2"/>
    <DependedModuleMetadata id="Sandbox" order="LoadBeforeThis"  version="1.1.2"/>
    <DependedModuleMetadata id="StoryMode" order="LoadBeforeThis" version="1.1.2"/>
  </DependedModuleMetadatas>
  <!-- Community Dependency Metadata -->
  <SubModules>
    <SubModule>
      <Name value="$modulename$" />
      <DLLName value="$modulename$.dll" />
      <SubModuleClassType value="$modulename$.SubModule" />
      <Tags />
    </SubModule>
  </SubModules>
</Module>
